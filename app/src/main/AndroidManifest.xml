<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ibrajix.jetpacker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_main_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_main_logo_round"
        android:supportsRtl="true"
        android:name="com.ibrajix.jetpacker.global.MyApplication"
        android:theme="@style/Theme.JetPacker">
        <activity
            android:name="com.ibrajix.jetpacker.ui.activities.ContainerActivity"
            android:exported="false" />
        <activity
            android:name="com.ibrajix.jetpacker.ui.activities.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>